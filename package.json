    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "babel-jest": "^25.1.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^25.1.0",
    "rollup": "^2.1.0",
